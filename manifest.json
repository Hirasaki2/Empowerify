{

  "manifest_version": 2,
  "name": "Empowerify",
  "version": "1.9.4.3",

  "description": "This tool adds a (Do you want to leave? and Redirect to old) to a new school website empower.",

  "icons": {
	  "128": "icons/icon.png"
  },

  "permissions": [
	  "storage"
  ],
  "options_ui": {
    "page": "html/options.html"
  },
  "browser_action": {
    "default_popup": "html/popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://*.empowerlearning.net/iFrame.aspx?iCtrl=*"],
      "js": ["js/jquery-3.3.1.min.js","js/empowerify.js"]
    },
    {
      "matches": ["https://*.empowerlearning.net/default.aspx?LOAD_PAGE=*"],
      "js": ["js/jquery-3.3.1.min.js","js/login.js"]
    }
  ],
  "applications": {
    "gecko": {
      "update_url": "https://example.com/updates.json"
    }
  }

}
