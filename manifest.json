    "48": "icons/icon.jpg"
  },
  "web_accessible_resources": [
    "icons/Canvas-Logo.png"
  ],

  "content_scripts": [
    {
      "matches": ["*://plano.empowerlearning.net/iFrame.aspx?iCtrl=*"],
      "js": ["empowerify.js"]
    }
  ]

}
