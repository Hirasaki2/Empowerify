{
  "manifest_version": 2,
  "name": "Empowerify",
  "version": "3.0.0",
  "description": "Fixing Empower Learning one function at a time",
  "icons": {
    "128": "src/icons/icon.png"
  },
  "background": {
    "scripts": [
      "src/js/background.js"
    ]
  },
  "permissions": [
    "storage",
    "*://*.empowerlearning.net/*",
    "activeTab",
    "webRequest",
    "webRequestBlocking"
  ],
  "options_ui": {
    "page": "src/html/options.html"
  },
  "browser_action": {
    "default_popup": "src/html/popup.html"
  }
}

